<div class="container well margin-top-20 small-container">	<%= form_for @course do |f| %>		<div class="bold margin-top-20">		Course name	</div>	<div class="margin-top-20">		<%= f.text_field :name %>	</div>	<div class="bold margin-top-20">		Course duration	</div>	<div class="margin-top-20">		<%= f.text_field :duration %>	</div>		<div class="bold margin-top-20">		Course difficulty	</div>	<div class="margin-top-20">		<%= f.text_field :difficulty %>	</div>			<div class="margin-top-20">		<a href="/students" class="btn btn-default">Cancel</a>		<button type="submit" class="btn btn-success">Save</button>	</div>	<% end %>	</div>